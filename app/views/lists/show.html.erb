<div class="container m-5">
<h1><%= @list.title %></h1>
<h2>Tasks</h2>
<% @tasks.each do |task| %>
<li><%= task.title %></li>
<% end %>
</div>

<div class="new task ms-5">
<%= link_to "Add a new task :)", new_list_task_path(@list)%>
</div>
<div class="overview ms-5 mt-2">
<%= link_to "Back to overview", lists_path %>
</div>
